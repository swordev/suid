{
  "name": "create-suid",
  "version": "0.2.1",
  "description": "CLI tool for creating a SUID project easily.",
  "keywords": [
    "cli",
    "suid",
    "solidjs"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json",
    "watch": "tsc --build -w tsconfig.build.json"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^11.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "x-wspa": {
    "pkgManifest": {
      "bin": "bin.js"
    }
  }
}

{
  "name": "@suid/styled-engine",
  "version": "0.8.0",
  "description": "Styled engine used by @suid/system.",
  "keywords": [
    "solid",
    "solidjs",
    "components",
    "mui",
    "styles",
    "css",
    "css-in-js"
  ],
  "main": "index.jsx",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json",
    "watch": "tsc --build -w tsconfig.build.json"
  },
  "dependencies": {
    "@suid/css": "workspace:*",
    "@suid/utils": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.8.18"
  },
  "publishConfig": {
    "directory": "lib"
  }
}
